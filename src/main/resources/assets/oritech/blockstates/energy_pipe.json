{
  "multipart": [
    {
      "apply": {
        "model": "oritech:block/generic_pipe_inner"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "oritech:block/generic_pipe_outer"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "oritech:block/generic_pipe_outer",
        "y": 180
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "oritech:block/generic_pipe_outer",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "oritech:block/generic_pipe_outer",
        "y": 270
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "oritech:block/generic_pipe_outer",
        "x": -90
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "oritech:block/generic_pipe_outer",
        "x": 90
      }
    }
  ]
}